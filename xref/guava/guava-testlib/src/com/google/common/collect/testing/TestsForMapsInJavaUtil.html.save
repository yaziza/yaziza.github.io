<a class="jxr_linenumber" name="L327" href="#L327">327</a>   <strong class="jxr_keyword">private</strong> <strong class="jxr_keyword">static</strong> <strong class="jxr_keyword">final</strong> <strong class="jxr_keyword">class</strong> NullFriendlyComparator&lt;T&gt; <strong class="jxr_keyword">implements</strong> Comparator&lt;T&gt;, Serializable {
