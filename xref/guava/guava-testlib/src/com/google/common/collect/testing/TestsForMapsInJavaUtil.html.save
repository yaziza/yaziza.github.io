<a class="jxr_linenumber" name="L313" href="#L313">313</a>   <em class="jxr_comment">// TODO: call conversion constructors or factory methods instead of using</em>
<a class="jxr_linenumber" name="L314" href="#L314">314</a>   <em class="jxr_comment">// populate() on an empty map</em>
<a class="jxr_linenumber" name="L315" href="#L315">315</a>   <strong class="jxr_keyword">private</strong> <strong class="jxr_keyword">static</strong> &lt;T, M <strong class="jxr_keyword">extends</strong> Map&lt;T, String&gt;&gt; M populate(
<a class="jxr_linenumber" name="L316" href="#L316">316</a>       M map, Entry&lt;T, String&gt;[] entries) {
<a class="jxr_linenumber" name="L317" href="#L317">317</a>     <strong class="jxr_keyword">for</strong> (Entry&lt;T, String&gt; entry : entries) {
<a class="jxr_linenumber" name="L318" href="#L318">318</a>       map.put(entry.getKey(), entry.getValue());
<a class="jxr_linenumber" name="L319" href="#L319">319</a>     }
