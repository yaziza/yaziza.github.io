<a class="jxr_linenumber" name="L60" href="#L60">60</a>      List&lt;E&gt; iteratorElements = <strong class="jxr_keyword">new</strong> ArrayList&lt;E&gt;();
<a class="jxr_linenumber" name="L61" href="#L61">61</a>      <strong class="jxr_keyword">for</strong> (E element : collection) { <em class="jxr_comment">// uses iterator()</em>
<a class="jxr_linenumber" name="L62" href="#L62">62</a>        iteratorElements.add(element);
<a class="jxr_linenumber" name="L63" href="#L63">63</a>      }
