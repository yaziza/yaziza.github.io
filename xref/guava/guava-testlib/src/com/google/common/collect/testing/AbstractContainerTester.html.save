<a class="jxr_linenumber" name="L37" href="#L37">37</a>  <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">abstract</strong> <strong class="jxr_keyword">class</strong> AbstractContainerTester&lt;C, E&gt;
<a class="jxr_linenumber" name="L38" href="#L38">38</a>      <strong class="jxr_keyword">extends</strong> AbstractTester&lt;OneSizeTestContainerGenerator&lt;C, E&gt;&gt; {
