<a class="jxr_linenumber" name="L240" href="#L240">240</a> 
<a class="jxr_linenumber" name="L241" href="#L241">241</a>     <em class="jxr_comment">// TODO(user): copy ImmutableSetMultimap by using asList() on the sets</em>
<a class="jxr_linenumber" name="L242" href="#L242">242</a>     <strong class="jxr_keyword">if</strong> (multimap instanceof ImmutableListMultimap) {
