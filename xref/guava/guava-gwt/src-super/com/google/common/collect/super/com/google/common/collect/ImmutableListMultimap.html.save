<a class="jxr_linenumber" name="L251" href="#L251">251</a>     ImmutableMap.Builder&lt;K, ImmutableList&lt;V&gt;&gt; builder = ImmutableMap.builder();
<a class="jxr_linenumber" name="L252" href="#L252">252</a>     <strong class="jxr_keyword">int</strong> size = 0;
<a class="jxr_linenumber" name="L253" href="#L253">253</a> 
<a class="jxr_linenumber" name="L254" href="#L254">254</a>     <strong class="jxr_keyword">for</strong> (Entry&lt;? <strong class="jxr_keyword">extends</strong> K, ? <strong class="jxr_keyword">extends</strong> Collection&lt;? <strong class="jxr_keyword">extends</strong> V&gt;&gt; entry
<a class="jxr_linenumber" name="L255" href="#L255">255</a>         : multimap.asMap().entrySet()) {
