<a class="jxr_linenumber" name="L488" href="#L488">488</a>     assertFalse(Iterables.elementsEqual(a, b));
<a class="jxr_linenumber" name="L489" href="#L489">489</a>     assertFalse(Iterables.elementsEqual(b, a));
<a class="jxr_linenumber" name="L490" href="#L490">490</a>   }
