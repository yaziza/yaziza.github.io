<a class="jxr_linenumber" name="L1204" href="#L1204">1204</a>   <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testMergeSorted_pyramid() {
<a class="jxr_linenumber" name="L1205" href="#L1205">1205</a>     List&lt;Iterable&lt;Integer&gt;&gt; iterables = Lists.newLinkedList();
<a class="jxr_linenumber" name="L1206" href="#L1206">1206</a>     List&lt;Integer&gt; allIntegers = Lists.newArrayList();
<a class="jxr_linenumber" name="L1207" href="#L1207">1207</a> 
<a class="jxr_linenumber" name="L1208" href="#L1208">1208</a>     <em class="jxr_comment">// Creates iterators like: {{}, {0}, {0, 1}, {0, 1, 2}, ...}</em>
<a class="jxr_linenumber" name="L1209" href="#L1209">1209</a>     <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> i = 0; i &lt; 10; i++) {
<a class="jxr_linenumber" name="L1210" href="#L1210">1210</a>       List&lt;Integer&gt; list = Lists.newLinkedList();
<a class="jxr_linenumber" name="L1211" href="#L1211">1211</a>       <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> j = 0; j &lt; i; j++) {
<a class="jxr_linenumber" name="L1212" href="#L1212">1212</a>         list.add(j);
<a class="jxr_linenumber" name="L1213" href="#L1213">1213</a>         allIntegers.add(j);
<a class="jxr_linenumber" name="L1214" href="#L1214">1214</a>       }
<a class="jxr_linenumber" name="L1215" href="#L1215">1215</a>       iterables.add(Ordering.natural().sortedCopy(list));
<a class="jxr_linenumber" name="L1216" href="#L1216">1216</a>     }
<a class="jxr_linenumber" name="L1217" href="#L1217">1217</a> 
<a class="jxr_linenumber" name="L1218" href="#L1218">1218</a>     verifyMergeSorted(iterables, allIntegers);
<a class="jxr_linenumber" name="L1219" href="#L1219">1219</a>   }
<a class="jxr_linenumber" name="L1220" href="#L1220">1220</a> 
<a class="jxr_linenumber" name="L1221" href="#L1221">1221</a>   <em class="jxr_comment">// Like the pyramid, but creates more unique values, along with repeated ones.</em>
<a class="jxr_linenumber" name="L1222" href="#L1222">1222</a>   <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testMergeSorted_skipping_pyramid() {
<a class="jxr_linenumber" name="L1223" href="#L1223">1223</a>     List&lt;Iterable&lt;Integer&gt;&gt; iterables = Lists.newLinkedList();
<a class="jxr_linenumber" name="L1224" href="#L1224">1224</a>     List&lt;Integer&gt; allIntegers = Lists.newArrayList();
<a class="jxr_linenumber" name="L1225" href="#L1225">1225</a> 
<a class="jxr_linenumber" name="L1226" href="#L1226">1226</a>     <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> i = 0; i &lt; 20; i++) {
<a class="jxr_linenumber" name="L1227" href="#L1227">1227</a>       List&lt;Integer&gt; list = Lists.newLinkedList();
<a class="jxr_linenumber" name="L1228" href="#L1228">1228</a>       <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">int</strong> j = 0; j &lt; i; j++) {
<a class="jxr_linenumber" name="L1229" href="#L1229">1229</a>         list.add(j * i);
<a class="jxr_linenumber" name="L1230" href="#L1230">1230</a>         allIntegers.add(j * i);
<a class="jxr_linenumber" name="L1231" href="#L1231">1231</a>       }
<a class="jxr_linenumber" name="L1232" href="#L1232">1232</a>       iterables.add(Ordering.natural().sortedCopy(list));
<a class="jxr_linenumber" name="L1233" href="#L1233">1233</a>     }
<a class="jxr_linenumber" name="L1234" href="#L1234">1234</a> 
<a class="jxr_linenumber" name="L1235" href="#L1235">1235</a>     verifyMergeSorted(iterables, allIntegers);
<a class="jxr_linenumber" name="L1236" href="#L1236">1236</a>   }
<a class="jxr_linenumber" name="L1237" href="#L1237">1237</a> 
<a class="jxr_linenumber" name="L1238" href="#L1238">1238</a>   <strong class="jxr_keyword">private</strong> <strong class="jxr_keyword">static</strong> <strong class="jxr_keyword">void</strong> verifyMergeSorted(Iterable&lt;Iterable&lt;Integer&gt;&gt; iterables,
