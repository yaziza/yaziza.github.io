<a class="jxr_linenumber" name="L530" href="#L530">530</a>   <strong class="jxr_keyword">private</strong> <strong class="jxr_keyword">interface</strong> <a href="../../../../com/google/common/collect/ListsTest.html">IntegerList</a> <strong class="jxr_keyword">extends</strong> List&lt;Integer&gt; {}
