<a class="jxr_linenumber" name="L532" href="#L532">532</a>   <strong class="jxr_keyword">private</strong> <strong class="jxr_keyword">static</strong> <strong class="jxr_keyword">void</strong> assertTransformIterator(List&lt;String&gt; list) {
<a class="jxr_linenumber" name="L533" href="#L533">533</a>     Iterator&lt;String&gt; iterator = list.iterator();
<a class="jxr_linenumber" name="L534" href="#L534">534</a>     assertTrue(iterator.hasNext());
<a class="jxr_linenumber" name="L535" href="#L535">535</a>     assertEquals(<span class="jxr_string">"1"</span>, iterator.next());
<a class="jxr_linenumber" name="L536" href="#L536">536</a>     assertTrue(iterator.hasNext());
<a class="jxr_linenumber" name="L537" href="#L537">537</a>     assertEquals(<span class="jxr_string">"2"</span>, iterator.next());
<a class="jxr_linenumber" name="L538" href="#L538">538</a>     assertTrue(iterator.hasNext());
<a class="jxr_linenumber" name="L539" href="#L539">539</a>     assertEquals(<span class="jxr_string">"3"</span>, iterator.next());
<a class="jxr_linenumber" name="L540" href="#L540">540</a>     assertTrue(iterator.hasNext());
<a class="jxr_linenumber" name="L541" href="#L541">541</a>     assertEquals(<span class="jxr_string">"4"</span>, iterator.next());
<a class="jxr_linenumber" name="L542" href="#L542">542</a>     assertFalse(iterator.hasNext());
<a class="jxr_linenumber" name="L543" href="#L543">543</a>     <strong class="jxr_keyword">try</strong> {
<a class="jxr_linenumber" name="L544" href="#L544">544</a>       iterator.next();
<a class="jxr_linenumber" name="L545" href="#L545">545</a>       fail(<span class="jxr_string">"did not detect end of list"</span>);
<a class="jxr_linenumber" name="L546" href="#L546">546</a>     } <strong class="jxr_keyword">catch</strong> (NoSuchElementException expected) {}
<a class="jxr_linenumber" name="L547" href="#L547">547</a>     iterator.remove();
<a class="jxr_linenumber" name="L548" href="#L548">548</a>     assertEquals(asList(<span class="jxr_string">"1"</span>, <span class="jxr_string">"2"</span>, <span class="jxr_string">"3"</span>), list);
<a class="jxr_linenumber" name="L549" href="#L549">549</a>     assertFalse(iterator.hasNext());
<a class="jxr_linenumber" name="L550" href="#L550">550</a>   }
