<a class="jxr_linenumber" name="L204" href="#L204">204</a>       fail(<span class="jxr_string">"no exception thrown"</span>);
<a class="jxr_linenumber" name="L205" href="#L205">205</a>     } <strong class="jxr_keyword">catch</strong> (UnsupportedOperationException expected) {
<a class="jxr_linenumber" name="L206" href="#L206">206</a>     }
<a class="jxr_linenumber" name="L207" href="#L207">207</a> 
<a class="jxr_linenumber" name="L208" href="#L208">208</a>     <em class="jxr_comment">// And it can't shrink</em>
