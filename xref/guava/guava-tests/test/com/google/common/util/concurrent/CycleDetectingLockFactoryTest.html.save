<a class="jxr_linenumber" name="L113" href="#L113">113</a>     } <strong class="jxr_keyword">catch</strong> (PotentialDeadlockException expected) {
<a class="jxr_linenumber" name="L114" href="#L114">114</a>       checkMessage(expected, <span class="jxr_string">"LockB -&gt; LockA"</span>, <span class="jxr_string">"LockA -&gt; LockB"</span>);
<a class="jxr_linenumber" name="L115" href="#L115">115</a>       firstException = expected;
<a class="jxr_linenumber" name="L116" href="#L116">116</a>     }
<a class="jxr_linenumber" name="L117" href="#L117">117</a> 
<a class="jxr_linenumber" name="L118" href="#L118">118</a>     <em class="jxr_comment">// Second time should also fail, with a cached causal chain.</em>
<a class="jxr_linenumber" name="L119" href="#L119">119</a>     <strong class="jxr_keyword">try</strong> {
<a class="jxr_linenumber" name="L120" href="#L120">120</a>       lockA.lock();
