<a class="jxr_linenumber" name="L151" href="#L151">151</a>   <em class="jxr_javadoccomment">/** Helper to manually escape a 7-bit ascii character */</em>
<a class="jxr_linenumber" name="L152" href="#L152">152</a>   <strong class="jxr_keyword">private</strong> String escapeAscii(<strong class="jxr_keyword">char</strong> c) {
<a class="jxr_linenumber" name="L153" href="#L153">153</a>     Preconditions.checkArgument(c &lt; 128);
<a class="jxr_linenumber" name="L154" href="#L154">154</a>     String hex = <span class="jxr_string">"0123456789ABCDEF"</span>;
<a class="jxr_linenumber" name="L155" href="#L155">155</a>     <strong class="jxr_keyword">return</strong> <span class="jxr_string">"%"</span> + hex.charAt((c &gt;&gt; 4) &amp; 0xf) + hex.charAt(c &amp; 0xf);
<a class="jxr_linenumber" name="L156" href="#L156">156</a>   }
