<a class="jxr_linenumber" name="L40" href="#L40">40</a>      <strong class="jxr_keyword">for</strong> (<strong class="jxr_keyword">char</strong> c = 0; c &lt; 128; c++) {
<a class="jxr_linenumber" name="L41" href="#L41">41</a>        <strong class="jxr_keyword">if</strong> ((c &gt;= '0' &amp;&amp; c &lt;= '9') ||
<a class="jxr_linenumber" name="L42" href="#L42">42</a>            (c &gt;= 'a' &amp;&amp; c &lt;= 'z') ||
<a class="jxr_linenumber" name="L43" href="#L43">43</a>            (c &gt;= 'A' &amp;&amp; c &lt;= 'Z')) {
<a class="jxr_linenumber" name="L44" href="#L44">44</a>          assertUnescaped(e, c);
<a class="jxr_linenumber" name="L45" href="#L45">45</a>        } <strong class="jxr_keyword">else</strong> {
<a class="jxr_linenumber" name="L46" href="#L46">46</a>          assertEscaping(e, escapeAscii(c), c);
<a class="jxr_linenumber" name="L47" href="#L47">47</a>        }
<a class="jxr_linenumber" name="L48" href="#L48">48</a>      }
<a class="jxr_linenumber" name="L49" href="#L49">49</a>  
<a class="jxr_linenumber" name="L50" href="#L50">50</a>      <em class="jxr_comment">// Testing mutlibyte escape sequences</em>
<a class="jxr_linenumber" name="L51" href="#L51">51</a>      assertEscaping(e, <span class="jxr_string">"%00"</span>, '\u0000');       <em class="jxr_comment">// nul</em>
<a class="jxr_linenumber" name="L52" href="#L52">52</a>      assertEscaping(e, <span class="jxr_string">"%7F"</span>, '\u007f');       <em class="jxr_comment">// del</em>
<a class="jxr_linenumber" name="L53" href="#L53">53</a>      assertEscaping(e, <span class="jxr_string">"%C2%80"</span>, '\u0080');    <em class="jxr_comment">// xx-00010,x-000000</em>
<a class="jxr_linenumber" name="L54" href="#L54">54</a>      assertEscaping(e, <span class="jxr_string">"%DF%BF"</span>, '\u07ff');    <em class="jxr_comment">// xx-11111,x-111111</em>
<a class="jxr_linenumber" name="L55" href="#L55">55</a>      assertEscaping(e, <span class="jxr_string">"%E0%A0%80"</span>, '\u0800'); <em class="jxr_comment">// xxx-0000,x-100000,x-00,0000</em>
<a class="jxr_linenumber" name="L56" href="#L56">56</a>      assertEscaping(e, <span class="jxr_string">"%EF%BF%BF"</span>, '\uffff'); <em class="jxr_comment">// xxx-1111,x-111111,x-11,1111</em>
<a class="jxr_linenumber" name="L57" href="#L57">57</a>      assertUnicodeEscaping(e, <span class="jxr_string">"%F0%90%80%80"</span>, '\uD800', '\uDC00');
<a class="jxr_linenumber" name="L58" href="#L58">58</a>      assertUnicodeEscaping(e, <span class="jxr_string">"%F4%8F%BF%BF"</span>, '\uDBFF', '\uDFFF');
<a class="jxr_linenumber" name="L59" href="#L59">59</a>  
<a class="jxr_linenumber" name="L60" href="#L60">60</a>      <em class="jxr_comment">// simple string tests</em>
<a class="jxr_linenumber" name="L61" href="#L61">61</a>      assertEquals(<span class="jxr_string">""</span>, e.escape(<span class="jxr_string">""</span>));
<a class="jxr_linenumber" name="L62" href="#L62">62</a>      assertEquals(<span class="jxr_string">"safestring"</span>, e.escape(<span class="jxr_string">"safestring"</span>));
<a class="jxr_linenumber" name="L63" href="#L63">63</a>      assertEquals(<span class="jxr_string">"embedded%00null"</span>, e.escape(<span class="jxr_string">"embedded\0null"</span>));
<a class="jxr_linenumber" name="L64" href="#L64">64</a>      assertEquals(<span class="jxr_string">"max%EF%BF%BFchar"</span>, e.escape(<span class="jxr_string">"max\uffffchar"</span>));
<a class="jxr_linenumber" name="L65" href="#L65">65</a>    }
