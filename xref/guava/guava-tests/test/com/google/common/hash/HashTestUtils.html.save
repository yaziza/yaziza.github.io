<a class="jxr_linenumber" name="L489" href="#L489">489</a>   <strong class="jxr_keyword">private</strong> <strong class="jxr_keyword">static</strong> <strong class="jxr_keyword">void</strong> assertHashBytesEquivalence(<a href="../../../../com/google/common/hash/HashFunction.html">HashFunction</a> hashFunction, Random random) {
