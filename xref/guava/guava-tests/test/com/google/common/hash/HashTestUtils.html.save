<a class="jxr_linenumber" name="L565" href="#L565">565</a>   <strong class="jxr_keyword">static</strong> <strong class="jxr_keyword">char</strong> randomLowSurrogate(Random random) {
<a class="jxr_linenumber" name="L566" href="#L566">566</a>     <strong class="jxr_keyword">return</strong> (<strong class="jxr_keyword">char</strong>) (Character.MIN_LOW_SURROGATE
<a class="jxr_linenumber" name="L567" href="#L567">567</a>         + random.nextInt(Character.MAX_LOW_SURROGATE - Character.MIN_LOW_SURROGATE + 1));
<a class="jxr_linenumber" name="L568" href="#L568">568</a>   }
<a class="jxr_linenumber" name="L569" href="#L569">569</a> 
<a class="jxr_linenumber" name="L570" href="#L570">570</a>   <strong class="jxr_keyword">static</strong> <strong class="jxr_keyword">char</strong> randomHighSurrogate(Random random) {
<a class="jxr_linenumber" name="L571" href="#L571">571</a>     <strong class="jxr_keyword">return</strong> (<strong class="jxr_keyword">char</strong>) (Character.MIN_HIGH_SURROGATE
<a class="jxr_linenumber" name="L572" href="#L572">572</a>         + random.nextInt(Character.MAX_HIGH_SURROGATE - Character.MIN_HIGH_SURROGATE + 1));
<a class="jxr_linenumber" name="L573" href="#L573">573</a>   }
