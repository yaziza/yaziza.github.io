<a class="jxr_linenumber" name="L45" href="#L45">45</a>    <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testKnownStringInputs() {
<a class="jxr_linenumber" name="L46" href="#L46">46</a>      assertHash(0, murmur3_32().hashUnencodedChars(<span class="jxr_string">""</span>));
<a class="jxr_linenumber" name="L47" href="#L47">47</a>      assertHash(679745764, murmur3_32().hashUnencodedChars(<span class="jxr_string">"k"</span>));
