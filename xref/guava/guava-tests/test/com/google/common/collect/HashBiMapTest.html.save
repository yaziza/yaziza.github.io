<a class="jxr_linenumber" name="L117" href="#L117">117</a>   <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testBiMapEntrySetIteratorRemove() {
<a class="jxr_linenumber" name="L118" href="#L118">118</a>     BiMap&lt;Integer, String&gt; map = HashBiMap.create();
<a class="jxr_linenumber" name="L119" href="#L119">119</a>     map.put(1, <span class="jxr_string">"one"</span>);
<a class="jxr_linenumber" name="L120" href="#L120">120</a>     Set&lt;Map.Entry&lt;Integer, String&gt;&gt; entries = map.entrySet();
<a class="jxr_linenumber" name="L121" href="#L121">121</a>     Iterator&lt;Map.Entry&lt;Integer, String&gt;&gt; iterator = entries.iterator();
<a class="jxr_linenumber" name="L122" href="#L122">122</a>     Map.Entry&lt;Integer, String&gt; entry = iterator.next();
<a class="jxr_linenumber" name="L123" href="#L123">123</a>     entry.setValue(<span class="jxr_string">"two"</span>); <em class="jxr_comment">// changes the iterator's current entry value</em>
<a class="jxr_linenumber" name="L124" href="#L124">124</a>     assertEquals(<span class="jxr_string">"two"</span>, map.get(1));
<a class="jxr_linenumber" name="L125" href="#L125">125</a>     assertEquals(Integer.valueOf(1), map.inverse().get(<span class="jxr_string">"two"</span>));
<a class="jxr_linenumber" name="L126" href="#L126">126</a>     iterator.remove(); <em class="jxr_comment">// removes the updated entry</em>
<a class="jxr_linenumber" name="L127" href="#L127">127</a>     assertTrue(map.isEmpty());
<a class="jxr_linenumber" name="L128" href="#L128">128</a>   }
