<a class="jxr_linenumber" name="L266" href="#L266">266</a>   <em class="jxr_javadoccomment">/**</em>
<a class="jxr_linenumber" name="L267" href="#L267">267</a> <em class="jxr_javadoccomment">   * Returns a "nefarious" collection, which permits only one call to</em>
<a class="jxr_linenumber" name="L268" href="#L268">268</a> <em class="jxr_javadoccomment">   * iterator(). This verifies that the constrained collection uses a defensive</em>
<a class="jxr_linenumber" name="L269" href="#L269">269</a> <em class="jxr_javadoccomment">   * copy instead of potentially checking the elements in one snapshot and</em>
<a class="jxr_linenumber" name="L270" href="#L270">270</a> <em class="jxr_javadoccomment">   * adding the elements from another.</em>
<a class="jxr_linenumber" name="L271" href="#L271">271</a> <em class="jxr_javadoccomment">   *</em>
<a class="jxr_linenumber" name="L272" href="#L272">272</a> <em class="jxr_javadoccomment">   * @param element the element to be contained in the collection</em>
<a class="jxr_linenumber" name="L273" href="#L273">273</a> <em class="jxr_javadoccomment">   */</em>
<a class="jxr_linenumber" name="L274" href="#L274">274</a>   <strong class="jxr_keyword">static</strong> &lt;E&gt; Collection&lt;E&gt; onceIterableCollection(<strong class="jxr_keyword">final</strong> E element) {
<a class="jxr_linenumber" name="L275" href="#L275">275</a>     <strong class="jxr_keyword">return</strong> <strong class="jxr_keyword">new</strong> AbstractCollection&lt;E&gt;() {
<a class="jxr_linenumber" name="L276" href="#L276">276</a>       <strong class="jxr_keyword">boolean</strong> iteratorCalled;
<a class="jxr_linenumber" name="L277" href="#L277">277</a>       @Override <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">int</strong> size() {
<a class="jxr_linenumber" name="L278" href="#L278">278</a>         <em class="jxr_comment">/*</em>
<a class="jxr_linenumber" name="L279" href="#L279">279</a> <em class="jxr_comment">         * We could make the collection empty, but that seems more likely to</em>
<a class="jxr_linenumber" name="L280" href="#L280">280</a> <em class="jxr_comment">         * trigger special cases (so maybe we should test both empty and</em>
<a class="jxr_linenumber" name="L281" href="#L281">281</a> <em class="jxr_comment">         * nonempty...).</em>
<a class="jxr_linenumber" name="L282" href="#L282">282</a> <em class="jxr_comment">         */</em>
<a class="jxr_linenumber" name="L283" href="#L283">283</a>         <strong class="jxr_keyword">return</strong> 1;
<a class="jxr_linenumber" name="L284" href="#L284">284</a>       }
<a class="jxr_linenumber" name="L285" href="#L285">285</a>       @Override <strong class="jxr_keyword">public</strong> Iterator&lt;E&gt; iterator() {
<a class="jxr_linenumber" name="L286" href="#L286">286</a>         assertFalse(<span class="jxr_string">"Expected only one call to iterator()"</span>, iteratorCalled);
<a class="jxr_linenumber" name="L287" href="#L287">287</a>         iteratorCalled = <strong class="jxr_keyword">true</strong>;
<a class="jxr_linenumber" name="L288" href="#L288">288</a>         <strong class="jxr_keyword">return</strong> Collections.singleton(element).iterator();
<a class="jxr_linenumber" name="L289" href="#L289">289</a>       }
<a class="jxr_linenumber" name="L290" href="#L290">290</a>     };
<a class="jxr_linenumber" name="L291" href="#L291">291</a>   }
