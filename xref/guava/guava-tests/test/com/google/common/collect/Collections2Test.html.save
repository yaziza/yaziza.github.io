<a class="jxr_linenumber" name="L478" href="#L478">478</a>       permutations.next();
<a class="jxr_linenumber" name="L479" href="#L479">479</a>       fail(<span class="jxr_string">"Expected NoSuchElementException."</span>);
<a class="jxr_linenumber" name="L480" href="#L480">480</a>     } <strong class="jxr_keyword">catch</strong> (NoSuchElementException expected) {}
<a class="jxr_linenumber" name="L481" href="#L481">481</a>   }
