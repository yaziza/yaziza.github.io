<a class="jxr_linenumber" name="L164" href="#L164">164</a>   <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testSingleBoundedRange() {
<a class="jxr_linenumber" name="L165" href="#L165">165</a>     ImmutableRangeSet&lt;Integer&gt; rangeSet = ImmutableRangeSet.of(Range.closedOpen(1, 5));
<a class="jxr_linenumber" name="L166" href="#L166">166</a> 
<a class="jxr_linenumber" name="L167" href="#L167">167</a>     assertThat(rangeSet.asRanges()).has().item(Range.closedOpen(1, 5));
<a class="jxr_linenumber" name="L168" href="#L168">168</a> 
<a class="jxr_linenumber" name="L169" href="#L169">169</a>     assertTrue(rangeSet.encloses(Range.closed(3, 4)));
<a class="jxr_linenumber" name="L170" href="#L170">170</a>     assertTrue(rangeSet.encloses(Range.closedOpen(1, 4)));
<a class="jxr_linenumber" name="L171" href="#L171">171</a>     assertTrue(rangeSet.encloses(Range.closedOpen(1, 5)));
<a class="jxr_linenumber" name="L172" href="#L172">172</a>     assertFalse(rangeSet.encloses(Range.greaterThan(2)));
<a class="jxr_linenumber" name="L173" href="#L173">173</a> 
