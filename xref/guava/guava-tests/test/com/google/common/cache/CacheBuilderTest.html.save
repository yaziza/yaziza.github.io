<a class="jxr_linenumber" name="L366" href="#L366">366</a>   @GwtIncompatible(<span class="jxr_string">"CacheTesting"</span>)
<a class="jxr_linenumber" name="L367" href="#L367">367</a>   <strong class="jxr_keyword">public</strong> <strong class="jxr_keyword">void</strong> testNullCache() {
<a class="jxr_linenumber" name="L368" href="#L368">368</a>     CountingRemovalListener&lt;Object, Object&gt; listener = countingRemovalListener();
<a class="jxr_linenumber" name="L369" href="#L369">369</a>     LoadingCache&lt;Object, Object&gt; nullCache = <strong class="jxr_keyword">new</strong> CacheBuilder&lt;Object, Object&gt;()
<a class="jxr_linenumber" name="L370" href="#L370">370</a>         .maximumSize(0)
<a class="jxr_linenumber" name="L371" href="#L371">371</a>         .removalListener(listener)
<a class="jxr_linenumber" name="L372" href="#L372">372</a>         .build(identityLoader());
<a class="jxr_linenumber" name="L373" href="#L373">373</a>     assertEquals(0, nullCache.size());
<a class="jxr_linenumber" name="L374" href="#L374">374</a>     Object key = <strong class="jxr_keyword">new</strong> Object();
<a class="jxr_linenumber" name="L375" href="#L375">375</a>     assertSame(key, nullCache.getUnchecked(key));
<a class="jxr_linenumber" name="L376" href="#L376">376</a>     assertEquals(1, listener.getCount());
<a class="jxr_linenumber" name="L377" href="#L377">377</a>     assertEquals(0, nullCache.size());
<a class="jxr_linenumber" name="L378" href="#L378">378</a>     CacheTesting.checkEmpty(nullCache.asMap());
<a class="jxr_linenumber" name="L379" href="#L379">379</a>   }
