<a class="jxr_linenumber" name="L348" href="#L348">348</a>     <strong class="jxr_keyword">int</strong> size = 0;
<a class="jxr_linenumber" name="L349" href="#L349">349</a>     <strong class="jxr_keyword">for</strong> (Segment&lt;?, ?&gt; segment : cchm.segments) {
