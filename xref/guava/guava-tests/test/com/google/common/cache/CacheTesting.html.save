<a class="jxr_linenumber" name="L363" href="#L363">363</a>   <strong class="jxr_keyword">static</strong> <strong class="jxr_keyword">void</strong> processPendingNotifications(Cache&lt;?, ?&gt; cache) {
<a class="jxr_linenumber" name="L364" href="#L364">364</a>     <strong class="jxr_keyword">if</strong> (hasLocalCache(cache)) {
<a class="jxr_linenumber" name="L365" href="#L365">365</a>       LocalCache&lt;?, ?&gt; cchm = toLocalCache(cache);
<a class="jxr_linenumber" name="L366" href="#L366">366</a>       cchm.processPendingNotifications();
