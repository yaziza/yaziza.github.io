<a class="jxr_linenumber" name="L354" href="#L354">354</a> 
<a class="jxr_linenumber" name="L355" href="#L355">355</a>   <strong class="jxr_keyword">static</strong> <strong class="jxr_keyword">int</strong> accessQueueSize(Segment&lt;?, ?&gt; segment) {
<a class="jxr_linenumber" name="L356" href="#L356">356</a>     <strong class="jxr_keyword">return</strong> segment.accessQueue.size();
<a class="jxr_linenumber" name="L357" href="#L357">357</a>   }
<a class="jxr_linenumber" name="L358" href="#L358">358</a> 
<a class="jxr_linenumber" name="L359" href="#L359">359</a>   <strong class="jxr_keyword">static</strong> <strong class="jxr_keyword">int</strong> expirationQueueSize(Cache&lt;?, ?&gt; cache) {
<a class="jxr_linenumber" name="L360" href="#L360">360</a>     <strong class="jxr_keyword">return</strong> Math.max(accessQueueSize(cache), writeQueueSize(cache));
